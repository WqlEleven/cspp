page{
  background: #f2f8fe;
  height: 100%;
}
.container {
  position: relative;
  width: 100%;
  color: #939393;
  margin-top: 24rpx;
  background: #f2f8fe;
}
.nav-left{
  color: #333333;
  width: 192rpx;
  background: #fff;
  text-align: center;
  float: left;
}
.nav-left .nav-left-items{
  height: 30px;
  line-height: 30px;
  padding: 6px 0;
  font-size: 30rpx;
  border-bottom-right-radius:10rpx; 
  border-top-right-radius:10rpx;
  position: relative;
}
.nav-left .nav-left-items .cicle{
  background-color: #bebebe;
  width: 6rpx;
  height: 6rpx;
  border-radius: 50%;
  position: absolute;
  top:40rpx;
  z-index: 1;
  left: 33rpx;
}
.nav-left .nav-left-items .arrow{
  width: 12rpx;
  height: 16rpx;
  position: absolute;
  top:35rpx;
  right: 12rpx;
  z-index: 1;
}
.nav-left .nav-left-items.active{
  background: #f2f8fe;
  color: #FF5A02;
  font-weight: bold;
  border-bottom-right-radius:0rpx; 
  border-top-right-radius:0rpx;
}
.nav-left .nav-left-items.active .cicle{
  background-color: #fff;
  width: 10rpx;
  height: 10rpx;
  top:40rpx;
  z-index: 1;
  left: 33rpx;
}
.nav_rightbox{
  width: 520rpx;
  background: #fff;
  float: left;
  margin-left: 20rpx;
  border-radius: 10rpx;
  color: #333333;
}
.nav_rightbox .nav_right_items{
  float: left;
  width: 33.33%;
  height: 80px;
  text-align: center;
  margin-bottom: 58rpx;
  margin-top: 30rpx;
}
.nav_rightbox .nav_right_items image{
  width: 118rpx;
  height: 118rpx;
}
.nav_rightbox .nav_right_items text{
  display: block;
  width: 82%;
  font-size: 28rpx;
  color: #333333;
  margin-left: 14rpx;
}
.search{
  background-color: #fff;
  padding: 26rpx 30rpx;
  position: relative;
  height: 80rpx;
}
.search input{
  background-color: #f1f1f1;
  border-radius: 50rpx;
  height: 72rpx;
  margin: 4rpx 4rpx 4rpx 4rpx;
  line-height: 72rpx;
  font-size: 30rpx;
  padding-left: 315rpx;
}
.search .btn-search{
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  left:294rpx;
  top:53rpx;
  z-index: 1;
}
.remen{
  height: 84rpx;
  width: 100%;
}
.remen .left{
  width: 192rpx;
  float: left;
  font-size: 30rpx;
  height: 84rpx;
  line-height: 84rpx;
  margin-top: 10rpx;
  font-weight: 500;
  color: #e94e6e;
  text-align: center;
}
.remen .right{
  margin-top: 10rpx;
  font-weight: 500;
  width: 538rpx;
  color: #333333;
  float: left;
  font-size: 30rpx;
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
}
.nogoods{
  text-align: center;
  padding-top:40rpx;

}