page{
  background-color: #f2f8fe;
  font-size: 32rpx;
  color: #333;
}
.index-nav{
  background-color: #fff;
  position: relative;
  font-size:26rpx;
  width: 100%;
}
.index-nav .dzjx_bg{
  width: 100%;
  height: 300rpx;
}
.shopkeeper{
  position: absolute;
  left:32rpx;
  top: 72rpx;
}
.shopkeeper .shoph{
  width: 156rpx;
  height: 156rpx;
}
.shopkeeper .shoph .head-img{
  display:block; 
  overflow:hidden; 
  border-radius: 50%;
  width: 156rpx;
  height: 156rpx;
}
.shopkeeper .shoph image{
  width: 156rpx;
  height: 156rpx;
}
.shopkeeper .shopmess{
  position: absolute;
  left:175rpx;
  width:300rpx;
  top: 46rpx;
  font-size: 26rpx;
  color: #fff;
  letter-spacing:3rpx;
}
.shopkeeper .shopmess text{
  display: block;
}
.shopkeeper .firname{
  font-size: 30rpx;
  color: #fff;
  letter-spacing:3rpx;
}
.shopkeeper .secname{
  color: #6a6a6a;
  font-size: 26rpx;
}
.shelves{
  background-color: #fff;
  height: 50rpx;
  width: 130rpx;
  position: absolute;
  top: 14rpx;
  right: 24rpx;
  line-height: 50rpx;
  text-align: center;
  border:solid 1px #cfcfcf;
  border-radius: 6rpx;
  font-size: 26rpx;
  color: #d30000;
}
.goodsList{
  margin: 10rpx 0;
}
.goods{
  background-color: #fff;
  height: 380rpx;
  margin-bottom: 20rpx;
}
.goods text{
  display: block;
}
.goodsPic{
  width: 210rpx;
  height: 210rpx;
  display: block;
  float: left;
  margin-left: 30rpx;
  margin-top: 40rpx;
  margin-right: 20rpx;
  border: 1rpx solid #eee;
  border-radius: 12rpx;
  padding: 4rpx ;
}
.goodsInfo{
  float: left;
}
.goodsName{
  width: 400rpx;
  font-size: 32rpx;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 38rpx;
}
.goodsDes{
  width: 400rpx;
  font-size: 28rpx;
  color: #666;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 8rpx;
}
.goods .goodsPrice{
  display: inline-block;
  height: 50rpx;
  color: #333;
  font-size: 36rpx;
  line-height: 50rpx;
}
.goods  text.goodsearn{
  display: inline-block;
  color: #c40000;
  font-size: 36rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin-left: 26rpx;
  margin-top: 58rpx;
}
text.goodsNum{
  font-size:24rpx;
  color: #666;
  height: 34rpx;
  line-height: 34rpx;
  margin-top: 6rpx;
}
.btn{
  height: 48rpx;
  text-align: center;
  font-size: 28rpx;
  color: #666;
  background-color: #eee;
  line-height: 48rpx;
  display: inline-block;
  width: 160rpx;
  margin-top: 14rpx;
  border-radius: 8rpx;
}
.btn-share{
  border: 1rpx solid #FF5A02 ;
  height: 48rpx;
  text-align: center;
  font-size: 28rpx;
  color: #FF5A02;
  background-color: #fff;
  line-height: 48rpx;
  display: inline-block;
  width: 112rpx;
  margin-top: 14rpx;
  border-radius: 8rpx;
  margin-left: 24rpx;
}
.color{
  background-color: #FF5A02;
  color: #fff;
}
.offFlag{
  width: 116rpx;
  height: 116rpx;
  position: absolute;
  top: 20rpx;
  right: 100rpx;
}
/*下架*/
.off{
  -webkit-filter: grayscale(100%);
    filter: gray;
}
.goods.off text,
.goods.off .goodsPrice,
.goods.off  text.goodsearn{
  color: #9e9e9e;
}
.off .btn{
  color: #9e9e9e;
  border: solid 1px #9e9e9e;
}
.goodsNull{
   padding: 40rpx 0;
    color: #999;
    text-align: center;
}
.goodsNullbox{
  margin-bottom:  120rpx;
}
.goodsNullbox{
  font-size: 32rpx;
  color: #333;
}
.goodsChoice {
  margin: 30rpx 0;
}
.choice[type="primary"]{
  margin:0 65rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background-color: #FF5A02;
  font-size: 32rpx;
  color: #fff;
}
.footer{
  background:#f9f9f9;
  color:#333;
  font-size: 24rpx;
  text-align: center;
  position: fixed;
  bottom:0;
  width: 100%;
  padding: 10rpx 0 5rpx;
}
.footer navigator{
  display: inline-block;
  width: 25%;
}
.footer navigator image{
  width: 50rpx;
  height: 50rpx;
  display: block;
  margin: 0 auto 8rpx;
}
.footer .cur{
  color: #1b1b1b
}
.blank{
  height: 97rpx
}