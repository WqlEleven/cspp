page {
  background-color: #f2f8fe;
}
.search{
  background-color: #fff;
  padding: 18rpx 30rpx;
  position: relative;
  height: 80rpx;
}
.search input{
  background-color: #f1f1f1;
  border-radius: 50rpx;
  height: 72rpx;
  margin: 4rpx 4rpx 4rpx 4rpx;
  line-height: 72rpx;
  font-size: 30rpx;
  padding-left: 315rpx;
}
.search .btn-search{
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  left:294rpx;
  top:43rpx;
  z-index: 1;
}

.tab {
  background-color: #fff;
  height:88rpx;
  line-height: 88rpx;
  text-align: center;
  font-size: 28rpx;
  color: #333;
}

.tabList {
  width: 250rpx;
  float: left;
}

.tabList.on {
  color: #FF5A02;
  border-bottom: 4rpx solid #FF5A02;
}
.goodsList{
  margin: 20rpx 0;
}

.goods{
  background-color: #fff;
  height: 380rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.goods text{
  display: block;
}
.goodsPic{
  width: 210rpx;
  height: 210rpx;
  display: block;
  float: left;
  margin-left: 30rpx;
  margin-top: 40rpx;
  margin-right: 20rpx;
  border: 1rpx solid #eee;
  border-radius: 12rpx;
  padding: 4rpx;
}
.goodsInfo{
  float: left;
}
.goodsName{
  width: 428rpx;
  font-size: 32rpx;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 38rpx;
}
.goodsDes{
  width: 428rpx;
  font-size: 28rpx;
  color: #666;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 8rpx;
}
.goods .goodsPrice{
  display: inline-block;
  height: 50rpx;
  color: #333;
  font-size: 36rpx;
  line-height: 50rpx;
}
.goods  text.goodsearn{
  display: inline-block;
  color: #c40000;
  font-size: 36rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin-left: 26rpx;
  margin-top: 18rpx;
}
.goods text.goodsNum{
  color: #666;
  font-size: 24rpx;
  height: 34rpx;
  line-height: 34rpx;
  margin-top: 10rpx;
  width: 142rpx;
}
.goods  text.goodsStock {
  color: #666;
  font-size: 24rpx;
  height: 34rpx;
  line-height: 34rpx;
  float: right;
  margin-top: -36rpx;
}
.btn{
  height: 48rpx;
  text-align: center;
  font-size: 28rpx;
  color: #666;
  background-color: #eee;
  line-height: 48rpx;
  display: inline-block;
  width: 160rpx;
  margin-top: 14rpx;
  border-radius: 8rpx;
}
.ubtn{
  height: 48rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  background-color: #FF5A02;
  line-height: 48rpx;
  display: inline-block;
  width: 160rpx;
  margin-top: 14rpx;
  border-radius: 8rpx;
}
.opration {
  position: absolute;
  bottom: 40rpx;
  right: 14rpx;
}
.offFlag {
  width: 116rpx;
  height: 116rpx;
  position: absolute;
  top: 20rpx;
  right: 100rpx;
}

/*下架*/

.off {
  -webkit-filter: grayscale(100%);
  filter: gray;
}

.goods.off text, .goods.off .goodsPrice, .goods.off  text.goodsearn {
  color: #9e9e9e;
}

.off .btn {
  background-color: #9e9e9e;
  color: #fff;
}

.goodsNull {
  color: #aaa;
  text-align: center;
  margin-top: 100rpx;
  font-size: 50rpx;
}

.serchbox .btn-search {
  display: inline-block;
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 5rpx;
}

.serchbox  .tip {
  text-align: center;
  font-size: 30rpx;
  padding: 75rpx 0 52rpx;
}

.popSearch {
  padding: 0 20rpx;
  text-align: center;
  display: inline-block;
}

.popSearch text {
  border: 1px solid #cecece;
  border-radius: 2px;
  padding: 16rpx 25rpx;
  word-wrap: normal;
  margin-top: 10rpx;
  display: inline-block;
}

.serchbox {
  text-align: center;
}

.addCar {
  width: 60rpx;
  margin-right: 20rpx;
}

/*加减数量*/

.cart-list .cart-count-box {
  position: absolute;
  bottom: 0rpx;
  width: 210rpx;
  height: 57rpx;
  overflow: hidden;
  right: 10rpx;
}

.cart-list .cart-count-box text {
  float: left;
  line-height: 50rpx;
  text-align: center;
  height: 52rpx;
}

.cart-count-down, .cart-count-add {
  float: left;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  text-align: center;
}

.cart-count-num {
  float: left;
  width: 70rpx;
  height: 50rpx;
  color: #636363;
  line-height: 50rpx;
  text-align: center;
  font-size: 30rpx;
}

.goToCart {
  position: fixed;
  bottom: 40rpx;
  right: 40rpx;
  height: 90rpx;
  width: 90rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #fff;
  border-radius: 50%;
}

.goToCart image {
  width: 100rpx;
}

.goToCart text {
  position: absolute;
  top: 6rpx;
  right: -10rpx;
  width: 40rpx;
  height: 40rpx;
  background-color: #c40000;
  border-radius: 50%;
  text-align: center;
  line-height: 40rpx;
  font-size: 24rpx;
  font-weight: bold;
}

.goodsInfo .spacial {
  color: #FF5A02 ;
  border: 1px solid #FF5A02 ;
  font-size: 20rpx;
  height: 32rpx;
  line-height: 32rpx;
  border-radius: 5rpx;
  text-align: center;
  width: 62rpx;
  margin-top: 4rpx;
}

.goodsInfo .spacialdis {
  color: #fff;
  font-size: 20rpx;
  padding: 4rpx 13rpx;
  background: #FF5A02 ;
  border-radius: 18rpx;
  margin-left: 10rpx;
}
