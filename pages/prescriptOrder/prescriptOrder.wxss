.weui-cell {
  padding: 10px 15px 5px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  align-items: center;
  border-bottom: 10rpx solid #f2f2f2;
}
.weui-cell_input {
  padding-top: 0;
  padding-bottom: 0;
}

.weui-uploader__hd {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding-bottom: 10px;
  align-items: center;
}
.weui-uploader__title {
  flex: 1;
}
.weui-uploader__info {
  color: #b2b2b2;
}
.weui-uploader__bd {
  margin-bottom: -4px;
  margin-right: -9px;
  overflow: hidden;
}
.weui-uploader__files {
  float: left;
  margin-bottom: 0rpx;
  position: relative;
}
.weui-uploader__file {
  float: left;
  margin-right: 18rpx;
  margin-bottom: 18rpx;
  position: relative;
}
.weui-uploader__img {
  display: block;
  width: 158rpx;
  height: 158rpx;
}
.blank{
  height: 116rpx;
}
.imgClear {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top: 0px;
  right: 0px;
  color: #fff;
  background-color: #ccc;
  text-align: center;
  line-height: 36rpx;
}
.weui-uploader__input-box {
  float: left;
  position: relative;
  margin-bottom: 9px;
  width: 154rpx;
  height: 54rpx;
  border: 1px solid #d9d9d9;
  padding-top: 100rpx;
  font-size: 26rpx;
  text-align: center;
}
.weui-uploader__input-box:before, .weui-uploader__input-box:after {
  content: " ";
  position: absolute;
  top: 30%;
  left: 50%;
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}
.weui-uploader__input-box:before {
  width: 2px;
  height: 60rpx;
}
.weui-uploader__input-box:after {
  width: 60rpx;
  height: 2px;
}
.weui-uploader__input {
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
