page{
  background-color: #f2f8fe;
}
.administration {
  width: 100%;
  position: relative;
  padding-bottom:100rpx;
}
.title{
  background-color: #c40000;
  color: #fff;
  padding: 0 37rpx;
  height: 76rpx;
  line-height: 76rpx;
  font-size: 30rpx;
}
.title image{
  width: 19rpx;
  height: 29rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 19rpx;
}
.administration_con {
background-color: #fff;
margin-top: 20rpx;
padding: 28rpx 30rpx 28rpx 34rpx;
}
.administration_con .list1 {
  color: #333;
  font-size: 32rpx;
  display: flex;
  position: relative;
  font-family: PingFang-SC-Medium;
}
.administration_con .name{
  width: 200rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: left;
}
.administration_con .list1 view:nth-child(3) {
  float: left;
}
.list1 .classname{
  width: 54rpx;
  height: 54rpx;
  position: absolute;
  top: 0;
  right: 70rpx;
}
.list1 .classname1{
  width: 54rpx;
  height: 54rpx;
  position: absolute;
  top: 0;
  right: 0rpx;
}
.list1 .classname image{
  width: 100%;
  height: 100%;
}
.list1 .classname1 image{
  width: 100%;
  height: 100%;
}
.administration_con .list2 {
  color: #333;
  font-size: 32rpx;
  padding: 5rpx 0;
  line-height: 44rpx;
  font-family: PingFang-SC-Regular;
  margin-top: 14rpx;
}
.label {
  overflow: hidden;
  display: flex;
  line-height: 30rpx;
  height: 30rpx;
}
.label icon {
  float: left;
}
.label .last {
  float: left;
}
.save-btn {
  width: 100%;
  height: 98rpx;
  font-size: 32rpx;
  text-align: center;
  line-height: 98rpx;
  background-color: #FF5A02 ;
  color: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  border-radius: 0;
}
.save-btn image{
  width: 44rpx;
  vertical-align:middle;
}
.save-btn:active {
  background-color: #FF5A02 ;
}
.noAddmanegr{
    text-align: center;
    padding:20rpx;
    font-size: 30rpx;
    color: #490012;
}
